<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <!-- JIT compilation for JavaScript -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>

    <!-- Allow unsigned executable memory for native addons -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>

    <!-- Disable library validation for development -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>

    <!-- Disable app sandbox for development (required for full system access) -->
    <key>com.apple.security.app-sandbox</key>
    <false/>

    <!-- Keychain access groups for SecureStore -->
   <!--  <key>keychain-access-groups</key>
