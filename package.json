    "ajv-cli": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-native-web": "^0.13.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
