    "electron-log": "^4.2.4",
    "electron-serve": "^1.0.0",
    "electron-updater": "^4.3.4",
    "expensify-common": "git+https://git@github.com:Expensify/expensify-common.git#bd55d39d47030dec01c287dbdebaff84beca3f35",
    "file-loader": "^6.0.0",
    "html-entities": "^1.3.1",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.merge": "^4.6.2",
    "lodash.orderby": "^4.6.0",
    "modal-enhanced-react-native-web": "^0.2.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
