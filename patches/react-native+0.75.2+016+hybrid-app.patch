+void Binding::schedulerDidFinishTransaction(
+    const MountingCoordinator::Shared& mountingCoordinator) {
+  auto mountingTransaction = mountingCoordinator->pullTransaction();
+  if (!mountingTransaction.has_value()) {
+    return;
