+  static auto scheduleMountItem = JFabricUIManager::javaClassStatic()
+                                      ->getMethod<void(
+                                          JMountItem::javaobject,
+                                          jint,
+                                          jlong,
+                                          jlong,
+                                          jlong,
+                                          jlong,
+                                          jlong,
+                                          jlong,
+                                          jlong,
+                                          jint)>("scheduleMountItem");
