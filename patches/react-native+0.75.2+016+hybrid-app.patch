+target_link_libraries(${CMAKE_PROJECT_NAME}
+        fabricjni                           # prefab ready
+        mapbufferjni                        # prefab ready
+        fbjni                               # via 3rd party prefab
+        folly_runtime                       # prefab ready
+        glog                                # prefab ready
+        jsi                                 # prefab ready
